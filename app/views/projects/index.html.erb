<div class="container">
  <div class="page-header">
    <%= link_to 'Create Project', new_project_path, class: "btn btn-info pull-right" %>
    <h1>Projects</h1>
  </div>
  <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tbody>
        <% @projects.each do |project| %>
        <tr>
          <td><%= link_to project.name, project_path(project) %></td>
        </tr>
        <% end %>
      </tbody>
  </table>
</div>
